{
    "manifest_version": 3,
    "name": "URL to QR code",
    "version": "1.0.0",
    "action": {
      "default_popup": "index.html"
    },
    "host_permissions": ["<all_urls>"],
    "description": "Translate URL to QR code.",
    "icons": {
        "16": "logo.png",
        "32": "logo.png",
        "48": "logo.png",
        "128": "logo.png"
    },
    "background": {
        "service_worker": "src/background/service-worker.js"
    }
  }
  